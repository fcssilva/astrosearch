lly(function () {\n        return _this.loading = false;\n      });\n    },\n    goToPerfil: function goToPerfil(userName) {\n      this.$router.push({\n        path: \"/perfil/\".concat(userName)\n      });\n    }\n  }\n};",{"version":3,"sources":["Home.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyCA,OAAA,KAAA,MAAA,OAAA;AACA,OAAA,MAAA,MAAA,qBAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,MADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA,GAFA;AAKA,EAAA,IALA,kBAKA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,UADA;AAEA,MAAA,OAAA,EAAA,KAFA;AAGA,MAAA,SAAA,EAAA;AAHA,KAAA;AAKA,GAXA;AAYA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,6BACA;AAAA;;AAEA,WAAA,SAAA,GAAA,EAAA;AAEA,UAAA,CAAA,KAAA,IAAA,CAAA,MAAA,EACA;AAEA,WAAA,OAAA,GAAA,IAAA,CAPA,CAQA;;AACA,MAAA,KAAA,CACA,GADA,CACA,qCADA,EACA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,CAAA,EAAA,KAAA;AADA;AADA,OADA,EAMA,IANA,CAMA,UAAA,QAAA;AAAA,eAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA;AAAA,OANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OATA,EAUA,OAVA,CAUA;AAAA,eAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AAAA,OAVA;AAWA,KArBA;AAsBA,IAAA,UAtBA,sBAsBA,QAtBA,EAsBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,oBAAA,QAAA;AAAA,OAAA;AACA;AAxBA;AAZA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    <Header />\n    <section class=\"search col\">\n        <div class=\"container\">\n            <h1 class=\"search__title\">\n                Digite o nome do usuário do Git-Hub\n            </h1>\n            \n            <form class=\"search__form\">\n                <input type=\"text\" class=\"search__input\" v-model=\"user\">\n                <button class=\"search__btn\" @click.prevent=\"searchUsersList\">Buscar</button>\n            </form>\n        </div>\n    </section>\n\n    <section class=\"loading\" v-if=\"loading\">\n        <div class=\"container\">\n            Carregando...\n        </div>\n    </section>\n\n    <section class=\"users\" v-if=\"usersList.items\">\n        <h3>Exibindo {{ usersList.total_count }} para {{ user }}</h3>\n        <div class=\"users-list\" v-if=\"usersList.items\">\n            <div class=\"users-list__box\" \n                v-for=\"(user, index) in usersList.items\" \n                :key=\"index\"\n                @click=\"goToPerfil( user.login )\">\n                <img class=\"users-list__logo\" :src=\"user.avatar_url\" >\n                <div class=\"users-list__info\">\n                    <h1 class=\"users-list__name\">{{ user.login }}</h1>\n                    <p class=\"users-list__url\">{{ user.url }}</p>\n                </div>\n            </div>\n        </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport Header from '@/components/Header'\n\nexport default {\n  name: 'home',\n  components: {\n    Header\n  },\n  data() {\n    return {\n      user: 'facebook',\n      loading: false,\n      usersList: []\n    }\n  },\n   methods: {\n      searchUsersList() {\n            \n         this.usersList = []\n\n         if(!this.user.length)\n            return\n               \n         this.loading = true\n         // Buscar usuários\n         axios\n            .get('https://api.github.com/search/users', {\n               params: {\n                  q: this.user\n               }\n            })\n            .then(response => this.usersList = response.data)\n            .catch(error=>{\n               console.log(error)\n            })\n            .finally(() => this.loading = false)\n      },\n      goToPerfil(userName) {\n         this.$router.push({ path: `/perfil/${userName}` })\n      }\n   }\n}\n</script>\n  \n<style>\n.search {\n    margin-bottom: 40px;\n    text-align: center;\n    padding: 20px;\n}\n\n.search__form {\n    display: flex;\n    flex-wrap: wrap;\n}\n\n.search__input {\n    height: 20px;\n    text-align: center; \n    width: 100%; \n    font-size: 1.0rem;\n    padding: 10px 15px;\n    color: #380021;\n    outline: none;\n    line-height: 1.5;\n    border: 1px solid #380021;\n}\n\n.search__btn {\n    background-color: #48335d;\n    padding: 10px 15px;\n    font-size: 1.1rem;\n    font-weight: 600;\n    color: #fff;\n    outline: none;\n    border: 0.5px solid #48335d;\n    box-shadow: none;\n    cursor: pointer;\n    width:100%;\n    margin-top: 10px;\n}\n\n.users-list {\n  display: flex;\n  flex-wrap: wrap;\n  flex-direction: row;\n  justify-content: space-between;\n  flex-wrap: wrap;\n}\n\n.users-list__box {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  align-items: center;\n  flex: 1 1 auto;\n  padding: 20px;\n  width: calc(100% - 40px);\n  border: 1px solid #ccc;\n  cursor: pointer;\n}\n\n.users-list__logo {\n  width: 80;\n  height: 80px;\n  padding: 10px;\n}\n\n.users-list__info {\n  display: flex;\n  flex-direction: column;\n  margin: 5px;\n}\n\n.users-list__name {\n  margin: 0;\n  padding: 0;\n  font-size: 1.5rem;\n}\n\n.users-list__url {\n  margin: 10px 0;\n  padding: 0;\n  font-size: 0.9rem;\n}\n\n@media (min-width: 361px) {\n    .search__form { flex-wrap: nowrap }\n    .search__input { text-align: left; width: 80%; font-size: 1.8rem; }\n    .search__btn { width:auto; margin-top: 0;}\n    .users-list__box {\n        flex-direction: row;\n        text-align: left;\n        padding: 5px;\n        flex: 1 1\n    }\n\n}\n</style>"],"sourceRoot":"src/views"}]}